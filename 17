#include <iostream>
using namespace std;
int main ()
{
int a[12];
int i=0;
int max=a[0];
cout << "Please enter the test scores: "<<endl;
for (i=0;i<12;i++)
    cin >> a[i];
for (i=0;i<12;i++){
    if (a[i]>max)
        max = a[i];
}
cout << "The highest score is: "<<max<<endl;

for (i=0;i<12;i++)


return 0;
}
